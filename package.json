{
  "name": "tensorflow-agent-service",
  "version": "1.0.0",
  "description": "AI Agent Service powered by TensorFlow.js with Pokemon tool capabilities - Deployed on Vercel",
  "main": "api/index.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "vercel-build": "echo 'Vercel build completed - ready for deployment'",
    "build": "echo 'No build step required for Vercel deployment'",
    "postinstall": "echo 'Dependencies installed successfully for Vercel'",
    "test": "node test/test_all.js",
    "test:all": "node test/test_all.js all",
    "test:unit": "node test/test_all.js unit",
    "test:integration": "node test/test_all.js integration",
    "test:performance": "node test/test_all.js performance",
    "test:security": "node test/test_all.js security",
    "test:api": "node test/test_all.js api",
    "test:agent": "node test/test_all.js agent",
    "test:pokemon": "node test/test_all.js pokemon",
    "test:original": "node test/test_agent.js",
    "test:watch": "nodemon test/test_all.js",
    "docker:build": "docker build -t tensorflow-agent-service .",
    "docker:run": "docker run -p 3000:3000 tensorflow-agent-service",
    "local:start": "node src/main.js",
    "local:test": "node test/test_all.js"
  },
  "keywords": [
    "tensorflow",
    "ai",
    "agent",
    "pokemon",
    "nodejs",
    "vercel",
    "serverless",
    "machine-learning",
    "neural-networks"
  ],
  "author": "AI Agent Developer",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.0",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "natural": "^6.6.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tensorflow-agent-service.git"
  },
  "homepage": "https://tensorflow-agent-service.vercel.app",
  "bugs": {
    "url": "https://github.com/your-username/tensorflow-agent-service/issues"
  },
  "vercel": {
    "functions": {
      "api/index.js": {
        "memory": 1024,
        "maxDuration": 30
      }
    }
  },
  "overrides": {
    "@tensorflow/tfjs-converter": "^4.20.0"
  },
  "npmrc": "legacy-peer-deps=true"
}
